{
  "name": "expo-detox-config-plugins",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:ios": "detox build -c ios",
    "test:ios": "detox test -c ios",
    "e2e:ios": "npm run build:ios && npm run test:ios",
    "build:android": "detox build -c android",
    "test:android": "detox test -c android",
    "e2e:android": "npm run build:android && npm run test:android"
  },
  "dependencies": {
    "@config-plugins/detox": "^3.0.0",
    "@types/react": "~18.0.0",
    "@types/react-native": "~0.69.1",
    "expo": "~46.0.9",
    "expo-status-bar": "~1.4.0",
    "react": "18.0.0",
    "react-native": "0.69.5",
    "typescript": "^4.6.3",
    "expo-splash-screen": "~0.16.2",
    "react-dom": "18.0.0",
    "react-native-web": "~0.18.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "detox": "^19.10.0",
    "jest": "^28.1.3"
  },
  "private": true
}
